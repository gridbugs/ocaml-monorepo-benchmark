Adds a new function to core to exercise rebuilding the monorepo after a change to a
library with many dependant libraries.

Apply this patch from duniverse/core.

diff --git a/core/src/list.ml b/core/src/list.ml
index d9c52b0..7e346a5 100644
--- a/core/src/list.ml
+++ b/core/src/list.ml
@@ -51,6 +51,8 @@ let slice a start stop =
   Ordered_collection_common.slice ~length_fun:length ~sub_fun:sub a start stop
 ;;
 
+let middle t = nth t ((length t) / 2)
+
 module Stable = struct
   module V1 = struct
     type nonrec 'a t = 'a t [@@deriving sexp, bin_io, compare]
diff --git a/core/src/list.mli b/core/src/list.mli
index e01159b..9f77b5d 100644
--- a/core/src/list.mli
+++ b/core/src/list.mli
@@ -101,6 +101,8 @@ val zip_with_remainder
   -> 'b list
   -> ('a * 'b) list * ('a list, 'b list) Either.t option
 
+val middle : 'a list -> 'a option
+
 module Stable : sig
   module V1 : sig
     type nonrec 'a t = 'a t [@@deriving sexp, bin_io, compare]
